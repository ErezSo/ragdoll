{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "arrow-body-style": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "no-unused-expressions": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  },
  "plugins": ["prettier"]
}
